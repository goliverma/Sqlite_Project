@model IEnumerable<Employee>
@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Address</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var item in Model)
      {
          <tr>
              <td>@item.Id</td>
              <td>@item.Name</td>
              <td>@item.Email</td>
              <td>@item.Address</td>
          </tr>
      }
    </tbody>
  </table>